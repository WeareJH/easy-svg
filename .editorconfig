# http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = "utf8"
insert_final_newline = true


[*.css]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true


[*.js]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true


[*.json]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true


[*.md]
trim_trailing_whitespace = false
